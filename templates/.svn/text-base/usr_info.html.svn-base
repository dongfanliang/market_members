{% extends "base_site.html" %}
{% block content %}
			<div class="conment-title">用户详细信息</div>
				<div class="conment-div">
					<div class="div">
						<form id="startform" action="" method="POST"> {% csrf_token %}
							<table width="100%" border="0" cellspacing="0" cellpadding="0">
								<tr>
									<td width="5" background="../media/images/table/table_l.gif"></td>
									<td>
										<table width="100%" border="0" cellspacing="0" cellpadding="0" class="tab">										
									<tbody>
										<tr class="bg-color-b">
											<th width="15%">用户名：</th>
											<td>
											<input name="name" readonly="readonly" value="{{ userinfo.user_userName }}" /input>
											</td>
										</tr>
										<tr class="bg-color-a">
											<th >用户邮箱 ：</td>
											<td>
												<input name="mail" readonly="readonly" value="{{ userinfo.user_email }}" /input>
 											</td>
										</tr> 
										<tr>
											<th class="bg-color-b">用户全名：</th>
											<td><input name="fullname" readonly="readonly" value="{{ userinfo.real_name }}" /input></td>
											
										</tr>
										<tr>
											<th  class="bg-color-a">公司名称：</th>
											<td>
											<input  name="affiliation" readonly="readonly" value="{{ userinfo.affiliation }}" /input>
											</td>
										</tr>
										<tr>
											<th class="bg-color-b">电话号码：</th>
											<td>
											<input name="telephone_number" readonly="readonly" value="{{ userinfo.telephone_number  }}" /input>
											</td>
										</tr>
										<tr>
											<th class="bg-color-a">项目描述：</th>
											<td>
											<textarea id="id_projectdescription" rows="5" cols="40" name="project_description" readonly="readonly"> {{ userinfo.project_description }} </textarea>
											</td>
										</tr>
									
									</tbody>
							</table>
						</td>
					
						<td width="5" background="../media/images/table/table_br.gif">&nbsp;</td></tr>
							<tr>
								<td width="5" height="28" background="../media/images/table/table_bl.gif"></td>
								<td background="../media/images/table/table_bc.gif" height="28">
								<div class="div_page">
									<div align="center" class="bn_bn">
								
                           <input name="button" onclick="javascript:history.go(-1);" type="button" class="switch_bn" value="返回" />
									</div>
								</div>
								</td>
								<td width="5" background="../media/images/table/table_br.gif">&nbsp;</td>
							</tr>
						</table>
					</form>
				</div>
			</div>
		</div>
	</div>
{% endblock %}
