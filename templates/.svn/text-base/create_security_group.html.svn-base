{% extends "base_site.html" %}
{% block content %}
				<div class="conment-title">创建安全组</div>
				
            <div class="conment-div">
            	
                <div class="div">
			<form action="" method="post">                	
                    
                        	<table width="100%" border="0" cellspacing="0" cellpadding="0" class="tab">                           
                              
                              <tbody>
            {% for field in form %}
       			<tr>
        			  <th width="15%">{{ field.label }}：</th>
        			  <td>{{ field }} {{ field.help_text }}		
					  {% for error in field.errors %} 
						 <span class="error"> {{error}}</span>
					  {% endfor %}
					  </td>
					</tr>
				{% endfor %}   
			      </tbody>
                            </table>
                        
                        	<div class="div_page">
                           
                                <div class="bn_bn">
                                  <button type="submit" onclick="" class="switch_bn">确定</button>
				  <button type="button" onclick="window.location.href='/groups/'" class="switch_bn">取消</button>
                                </div>
                            </div>
                        
			</form>
                    
                    
                    
                </div>
                
            </div>
            
        </div>
{% endblock %}

