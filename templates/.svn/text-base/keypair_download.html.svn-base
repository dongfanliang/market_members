{% extends "base.html" %}
{% block content %}

<div class="conment">
	<div class="conment-title">下载密钥</div>
		<div class="conment-div">
			<div class="div">
			  <form id="downloadForm" action="" method="POST" enctype="multipart/form-data">{% csrf_token %}     	
				
                        	<table width="100%" border="0" cellspacing="0" cellpadding="0" class="tab">                            
								
                              <tbody>
                                  <tr>
									<th width="15%">密钥对名称：</th>
									<td>{{keyname}}</td>
								  </tr>
                                  <tr>
                                    <th>另存为：</th>
                                    <td><input type="text" id="save_file" name="save_file" value="{{ save_file }}"/><span style="color:red;FONT-SIZE:12px;margin-left:10px;">请指定保存的路径（目路＋文件名，如：/tmp/keypair1.private）</span></td>
                                  </tr>
                              </tbody>
							</table>                        
                        	<div class="div_page">                           
                                <div class="bn_bn">
                                  <button type="submit" onclick="" class="switch_bn">下载</button>
											<button type="back" onclick="window.location.href='/keypairs'" class="switch_bn">取消</button>
                                </div>
                            </div>
                        
				  </form>
                </div>            
            </div>
      </div>
    </div>
{% endblock %}
