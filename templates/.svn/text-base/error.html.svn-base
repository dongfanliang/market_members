<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
<title>方德高可信云平台管理工具</title>
<link href="../media/css/main.css" rel="stylesheet" type="text/css" />
<!–[if IE 6]> 
<script type="text/javascript" src="../media/js/0.0.8a-min.js"></script> 
<script> 
	DD_belatedPNG.fix('.logo, img');  
</script> 
<![end if]–> 
<script src="../media/js/jquery/jquery-1.3.2.min.js" language="javascript"></script>
<script src="../media/js/window.js" language="javascript"></script>
<script src="../media/js/cookie.js" language="javascript"></script>
<script type="text/javascript">
var t;
t=setTimeout("logout_auto()",10*60000);
function logout_auto()
{
	logout();
	location.href="/logout";
}
function goindex()
{ 
clearTimeout(t); 
t=setTimeout("logout_auto()",10*60000); 
}
function logout()
{
   deleteCookie("menu2","/");
	deleteCookie("menu3","/");
	deleteCookie("menu6","/");
	deleteCookie("menu8","/");

}
</script>
</head>
<body onkeypress="goindex()"  onmousedown="goindex()" onmousemove="goindex()" onclick="goindex()">
<div class="wrap">
	<div class="header">
    	<div class="logo"><img src="../media/images/logo.png" /></div>
        <div class="logo-font"><span>方德高可信云平台管理工具</span></div>
    	<div class="header-right">
        </div>
    </div>
    <div class="main">
      <div class="conment">
   	  <div class="conment-title">错误处理</div>
			<div class="conment-div">
			<form id="errorform" method="post">{%csrf_token%}
			
               <div class="error-div">
               			<h1>出错信息</h1>
               		<p class="error-div">
               			<h2 class="error">&nbsp;&nbsp;错误说明：</h2>
               			<br><h3>&nbsp;&nbsp;&nbsp;&nbsp;{{ error }}<h3>
               		{% if except %}
               			<br><h2 class="error">&nbsp;&nbsp;详细信息：</h2>           		
               		    <br><h3>&nbsp;&nbsp;&nbsp;&nbsp;{{ except }}</h3>
             		    {% endif%}
               		</p>
               		
               </div>
               
              		<div align="left" class="div_page">
					{% if page %}
						 &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<input type="button" onclick="window.location.href={{ page }};" class="switch_bn" value="返回"></input>    
						 {% else %}
						  &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<input type="button" onclick="javascript:history.go(-1);" class="switch_bn" value="返回"></input>
						  {% endif %}
                        </div>
               </form>
            </div>
            
      </div>
  </div>
    <div class="footer">信息显示</div>
</div>
</body>
</html>
