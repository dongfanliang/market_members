 {% extends "base_site.html" %}  
 {% block content %}
		<div class="conment">
        	<div class="conment-title">
            	<table border="0" cellspacing="0" cellpadding="0">
                  <tr>
						<td class="refresh"><a href="/ippool/">刷新</a></td>
						<td class="fpimg"><a href="/ippool_allocate/">分配IP</a></td>
						<td class="sfimg"><a href="#" onclick="show_release('cb','ippoolform','/ippool_release/')">释放IP</a></td>

						<td class="glimg"><a onclick="show_associate('cb','ippoolform','/ippool_associate/');return false;" href="#">关联IP</a></td>

						<td class="qximg"><a onclick="show_disassociate('cb','ippoolform','/ippool_disassociate/');return false;" href="#">取消关联IP</a></td>
                  </tr>
                </table>            	
            </div>
            
        <div class="conment-div">
			<div class="div">
				<form id="ippoolform" action="" method="post"> {% csrf_token %} 
                    <table width="100%" border="0" cellspacing="0" cellpadding="0">
                      <tr>
                        <td width="5" background="../media/images/table/table_l.gif"></td>
                        <td>
							<table width="100%" border="0" cellspacing="0" cellpadding="0" class="table">
								<thead>
                                    <tr><th width="4%"><div class="th_div"><span class="rt"></span></div></th>
 
                                    <th width="30%"><span>IP地址</span></th>
                                    <th width="30%">Instance ID</th>
								<tbody>
									{% if addresses_list %}
									{% for addresse in addresses_list.object_list %}
									<tr >
										<td class="txc"><input name="cb" type="checkbox" value="{{addresse.public_ip}} {{addresse.instance_id}}" onclick="selectOne(this)"/></td>

										<td align="center" valign="middle">{{ addresse.public_ip }}</td>
										<td align="center" valign="middle">{{ addresse.instance_id}}</td>
									</tr>
									{% endfor %}
									{% endif%}
		                        </tbody>
							</table>
						  <input name="savedata" type="hidden" value=""/>
                        </td>
                        <td width="5" background="../media/images/table/table_r.gif"></td>
                      </tr>
                      <tr>
                        <td width="5" height="28" background="../media/images/table/table_bl.gif"></td>
                        <td background="../media/images/table/table_bc.gif" height="28">
                        <div class="div_page">
                            	<div class="total">共有 <span>{{ num }}</span> 条记录，<span class="current">当前第 <b>
{% if addresses_list %}
{{addresses_list.number}}/{{addresses_list.paginator.num_pages}}
{% else%}
1/1
{% endif %}
</b> 页 </span></div>
 {% if not page_num == 1%}              
                <div class="bn_bn">
{% if addresses_list %}
  {% ifequal addresses_list.number 1 %}
	<span class="color_a">首页</a></span>
  {% else %}
    <span class="color_a"><a href="?page=1">首页</a></span>
  {% endifequal %}
{% endif %}

{% if addresses_list.has_previous %}
<a href="?page={{ addresses_list.previous_page_number }}">上一页</a>
{% endif %}

<span>
{% for p in page_range %}
  {% if addresses_list %}
	  {% ifequal p addresses_list.number %}
	   <span> {{ p }} </span>
	  {% else %}
	    <a href="?page={{ p }}" title="第{{ p }}页">{{p}}</a>
	  {% endifequal %}
  {% else %}
          <span> {{ p }} </span>
  {% endif %}
{% endfor %}
</span>

{% if addresses_list.has_next %}
   <a href="?page={{ addresses_list.next_page_number }}">下一页</a>
   <a href="?page={{addresses_list.paginator.num_pages}}">尾页</a>
{% else%}
   <span class="color_a">尾页&nbsp;</a></span>   
{% endif %}


{% if addresses_list %}
第<input name="goto" id="goto" class="input" type="text" style="ime-mode:Disabled" onkeydown=" return goto_page(event)"/>页<input type="button" class="switch_bn" name="turn_page" id="turn_page" value="转到" onclick="javascript:GoPage('goto',{{ addresses_list.paginator.num_pages }},'/ippool/')"/>
{% else %}
到第<input name="goto" id="goto" class="input" type="text" style="ime-mode:Disabled" onkeydown=" return goto_page(event)"/>页<input type="button" class="switch_bn" name="turn_page" id="turn_page" value="转到" onclick="javascript:GoPage('goto',1,'/ippool/')"/>
{% endif %}
</span>
		</div>
		{% endif %}

								<div class="clear"></div>
                            </div>
                        </td>
                        <td width="5" background="../media/images/table/table_br.gif">&nbsp;</td>
                      </tr>
                    </table>
			</form>
        </div>
    </div>
</div>
{% endblock %} 
